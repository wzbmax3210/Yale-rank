.log-list {
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
}
.page-section {
  width: 100%;
}

.weui-cells__title {
  margin-top: 15rpx;
  margin-bottom: 6rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
  color: #999999;
  font-size: 28rpx;
}
.weui-cells {
  border-bottom: 2rpx solid #d9d9d9;
  position: relative;
  background-color: #FFFFFF;
  font-size: 34rpx;
}
.weui-cell {
  padding: 10rpx 30rpx;
  margin-bottom: 30rpx;
  position: relative;
  display: flex;
  align-items: center;
}
.weui-input {
  height: 60rpx;
  min-height: 60rpx;
  line-height: 60rpx;
  border: 2rpx solid #d9d9d9;
  width: 100%;
}
