/**index.wxss**/
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #aaa;
}

.userinfo-avatar {
  overflow: hidden;
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.usermotto {
  margin-top: 200px;
}

.yale-header {
  width: 100%;
  box-sizing: border-box;
  font-size: 36rpx;
  height: 190rpx;
  padding: 20rpx 34rpx 30rpx;
  margin: 0;
  color: #ffffff;
  background: #706CAA;
  text-align: center;
}
.yale-header .update-button {
  position: absolute;
  color: #706CAA;
  width: 30rpx;
  height: 30rpx;
  right: 10rpx;
  top: 80rpx;
  border: none;
}

.yale-content {
  width: 100%;
  box-sizing: border-box;
  font-size: 28rpx;
  padding: 0 34rpx;
}

.top-3-background-color {
  background: #fff9f3;
}

.top-1-color {
  color: #FE2D46;
}

.top-2-color {
  color: #FF6600;
}

.top-3-color {
  color: #FAA90E;
}

.title-color {
  color: #666666;
}

.rank-wrapper {
  display: flex;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 2rpx solid #f6f3f0;
  flex-basis: 100%;
  text-align: center;
}

.rank-sort {
  width: 10%;
}

.rank-name {
  width: 20%;
}

.rank-point {
  width: 23%;
}

.last-rank {
  width: 22%;
}

.last-rank-up {
  color: #F73131;
}
.last-rank-down {
  color: #0EBE98;
}

.rank-title {
  width: 25%;
}
.fortune-god {
  animation: fortune-animation 1s infinite;
  animation-direction: alternate;
  color: #FFD700;
}
.mahjong-god {
  animation: mahjong-animation 1s infinite;
  animation-direction: alternate;
  color: #FFC0CB;
}
@keyframes fortune-animation {
  0% {
    color: #FFD700;
  }
  100% {
    color: #B8860B;
  }
}
@keyframes mahjong-animation {
  0% {
    color: #FFC0CB;
  }
  100% {
    color: #BC1717;
  }
}
.match {
  width: 100%;
  margin-top: 24rpx;
  background: #f5f5f5;
}
.match-list {
  background: #FFFFFF;
  border-radius: 10rpx;
  margin: 34rpx;
  box-shadow: 0 1px 3px #1a1a1a33;
}
.match-date {
  height: 72rpx;
  line-height: 72rpx;
  font-size: 26rpx;
  color: #333333;
  padding: 0 48rpx;
}
.match-detail-wrapper {
  height: 170rpx;
  font-size: 24rpx;
  padding: 0 96rpx;
}
.match-detail {
  position: relative;
  display: inline-block;
  line-height: 80rpx;
  width: 50%;
}
